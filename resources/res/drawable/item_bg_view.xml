<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true">
        <shape>
            <stroke
                android:width="0.5dp"
                android:color="@color/text_press_gray"/>
            <solid android:color="@color/view_gray"/>
        </shape>
    </item>
    <item>
        <shape>
            <solid android:color="@color/text_white"/>
            <stroke
                android:width="0.5dp"
                android:color="@color/view_gray"/>
        </shape>
    </item>
</selector>
